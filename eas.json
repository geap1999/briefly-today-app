{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
      "env": { 
        "EXPO_DEBUG": "true",
        "EXPO_PUBLIC_ENV": "development"
      },
			"android": {
				"gradleCommand": ":app:assembleDebug -x lint -x lintVitalAnalyzeRelease"
			}
		},
		"preview": {
			"distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview"
      }
		},
		"production": {
			"autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
			"android": {
				"buildType": "app-bundle",
				"gradleCommand": ":app:bundleRelease -x lint -x lintVitalAnalyzeRelease"
			}
		},
		"production-apk": {
			"autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      },
			"android": {
				"buildType": "apk",
				"gradleCommand": ":app:assembleRelease -x lint -x lintVitalAnalyzeRelease"
			}
		}
	},
	"submit": {
		"production": {}
	}
}